# Databricks workspace URL and token for DBSQL MCP server
export DATABRICKS_WORKSPACE_URL=https://your-workspace.cloud.databricks.com
export DATABRICKS_TOKEN=<PAT TOKEN>



# Databricks Connect Configuration (for local development only)
# When running in Databricks workspace, these are not needed

# Option 1: Use profile + serverless (recommended for most cases)
#export DATABRICKS_CONFIG_PROFILE=<profile in .databrickscfg>
export DATABRICKS_SERVERLESS_COMPUTE_ID=auto

# Option 2: Use profile + specific cluster
# export DATABRICKS_CONFIG_PROFILE=<profile in .databrickscfg>
# export DATABRICKS_CLUSTER_ID=<your-cluster-id>


# Usage:
# source .env  # Load environment variables
# python sample_pyspark.py  # Run the script
